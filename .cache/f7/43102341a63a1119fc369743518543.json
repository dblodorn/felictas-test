(scene, camera);\n  }\n\n  start() {\n    loop.start();\n  }\n\n  stop() {\n    loop.stop();\n  }\n\n  async init() {\n    const { model } = await loadModels(\n      \"https://media.dmbk.io/fr-models/Segments-Materials-r1b.gltf\"\n    );\n    scene.add(model);\n  }\n}\n\nexport { World };\n"},"lineCount":null}},"error":null,"hash":"0bbc2ab5044c01073ef6a92bb8bc99d6","cacheData":{"env":{}}}